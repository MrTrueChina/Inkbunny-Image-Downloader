{
  "name": "Inkbunny Image Downloader",
  "description": "Inkbunny Image Downloader",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/inkbunny_image_downloader16.png",
      "32": "/images/inkbunny_image_downloader32.png",
      "48": "/images/inkbunny_image_downloader48.png",
      "128": "/images/inkbunny_image_downloader128.png"
    }
  },
  "icons": {
    "16": "/images/inkbunny_image_downloader16.png",
    "32": "/images/inkbunny_image_downloader32.png",
    "48": "/images/inkbunny_image_downloader48.png",
    "128": "/images/inkbunny_image_downloader128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "https://inkbunny.net/s/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}